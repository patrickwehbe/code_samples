ffect(() => {\n    const getTotal = () => {\n      var t = 0;\n      items.forEach((item) => {\n        orderProducts.map((product) => {\n          if (item.product_id == product.product_id) {\n            t += product.product_price * item.order_item_quantity;\n          }\n        });\n        setTotal(t);\n      });\n    };\n    getTotal();\n  }, [items, orderProducts]);\n  if (orderProducts.length === 0) return null;\n  return (\n    <div style={{ paddingTop: '20vh', marginLeft: '5vw' }}>\n      <div style={{ marginLeft: '20px', marginBottom: '30px' }}>\n        {orderStatus === 'c' ? (\n          <h2>Order is completed</h2>\n        ) : orderStatus === 'a' ? (\n          <h2>Order is accepted</h2>\n        ) : orderStatus === 'd' ? (\n          <h2>Order is declined</h2>\n        ) : orderStatus === 'p' ? (\n          <h2>Order is pending</h2>\n        ) : null}\n      </div>\n      <Grid container>\n        {orderProducts.map((product) => (\n          <Grid item xs={12} sm={6} lg={4} md={4}>\n            {items.map((item) => {\n              if (item.product_id == product.product_id) {\n                return (\n                  <Product\n                    key={product.product_id}\n                    product={product}\n                    item={item}\n                  />\n                );\n              }\n            })}\n          </Grid>\n        ))}\n      </Grid>\n      <h1>Total= {total} L.L. </h1>\n    </div>\n  );\n}\n\nexport default OrderDetails;\n","/Users/patrickwehbe/Desktop/nitchd-web/frontend/src/Components/ProviderUserInfo/ProviderUserInfo.js",["435","436","437"],"import React, { useEffect, useState } from \"react\";\nimport axios from \"../../axios\";\n\nfunction ProviderUserInfo() {\n  const user_id = JSON.parse(localStorage.getItem(\"uu\"));\n  const [username, setUsername] = useState(\"\");\n  const [phoneNum, setPhoneNum] = useState(\"\");\n  const [email, setEmail] = useState(\"\");\n  const [gender, setGender] = useState(\"\");\n\n